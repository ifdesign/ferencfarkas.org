{
  "name": "ferencfarkas.org",
  "version": "0.1.0",
  "description": "The ferencfarkas.org project",
  "private": true,
  "scripts": {
    "start": "npm run website:serve",
    "dev": "run-p webapp:serve website:serve",
    "prebuild": "node ./scripts/prebuild-web",
    "webapp:serve": "cd webapp && npm run dev",
    "webapp:build": "cd webapp && npm run build && cd ..",
    "website:serve": "npm run prebuild -- dev && cd ./website && hugo server",
    "catalogue:pdf": "node ./catalogue/pdf-generator/index.js"
  },
  "dependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.12",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "autoprefixer": "^10.2.5",
    "date-fns": "^2.19.0",
    "eslint": "^7.22.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-standard": "^4.1.0",
    "front-matter": "^4.0.2",
    "fs-extra": "^9.1.0",
    "isomorphic-git": "^1.8.1",
    "lunr": "^2.3.9",
    "lunr-languages": "^1.4.0",
    "pdfmake": "^0.1.70",
    "postcss-cli": "^8.3.1",
    "postcss-custom-media": "^8.0.0",
    "prettier": "^2.2.1",
    "rollup": "^2.42.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "shelljs": "^0.8.4",
    "sirv-cli": "^1.0.11",
    "standard": "^16.0.3",
    "svelte": "^3.35.0",
    "wavesurfer.js": "^4.6.0",
    "yaml": "^1.10.2"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
